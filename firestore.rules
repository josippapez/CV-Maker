rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }
    match /users/{userId}{
    	allow create
      allow read: if request.auth.uid != null
      allow write, update: if request.auth.uid == userId
    }
    match /user-data/{userId}{
    	allow create
      allow read: if request.auth.uid != null
      allow write, update: if request.auth.uid == userId
    }
  }
}
